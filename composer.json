{
    "name": "ttek/btc",
    "type": "project",
    "description": "A site to track basic bitcoin trends",
    "keywords":  [
        "tropotek"
    ],
    "time": "2022-11-16",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors":  [
         {
            "name": "Michael Mifsud",
            "homepage": "http://www.tropotek.com/"
        }
    ],
    "autoload":  {
        "psr-0":  {
            "": "src/"
        }
    },
    "scripts":  {
        "post-install-cmd":  [
            "Tk\\Composer\\SetupEvent::postInstall"
        ],
        "post-update-cmd":  [
            "Tk\\Composer\\SetupEvent::postUpdate"
        ]
    },
    "extra":  {
        "branch-alias":  {
            "dev-master": "3.0.x-dev"
        }
    },
    "require":  {
        "php": "^7.3.0",
        "ttek/tk-base": "~3.4.0",
        "ttek/theme-cube": "~1.4.0",
        "ttek-plg/mailog": "~3.0.0",
        "ccxt/ccxt": "~1.58.0",
        "spomky-labs/otphp": "v10.0.x-dev"
    },
    "config":  {
        "allow-plugins":  {
            "ttek/tk-installers": true
        },
        "preferred-install":  {
            "ttek/*": "source"
        }
    }
}